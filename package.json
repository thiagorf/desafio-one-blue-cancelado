{
  "name": "one-blue",
  "version": "1.0.0",
  "description": "one blue api challenge",
  "main": "index.js",
  "repository": "git@gitlab.com:thiagorf/one-blue.git",
  "author": "thiagorf <trfarciroli@gmail.com>",
  "license": "MIT",
  "scripts": {
	"dev": "ts-node-dev --respawn --transpile-only --exit-child src/server.ts",
	"dev:migration": "prisma migrate dev",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "format": "prettier --check ./src",
    "container:stop": "docker container stop $(docker ps -q)",
    "compose:dev": "docker compose up -d",
    "create:migration": "docker compose up -d && yarn prisma migrate dev --create-only",
    "docker:dev": "yarn compose:dev && yarn prisma migrate deploy && yarn ts-node-dev --respawn --transpile-only --exit-child src/server.ts"
    
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "@types/express": "^4.17.17",
    "bcrypt": "^5.1.0",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "ioredis": "^5.3.0",
    "prisma": "^4.9.0",
    "swagger-autogen": "^2.23.0",
    "swagger-ui-express": "^4.6.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.19",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.4.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0"
  }
}
